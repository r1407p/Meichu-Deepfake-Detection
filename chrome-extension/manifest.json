{
    "name": "AI Image Detector",
    "version": "0.0.3",
    "manifest_version": 3,
    "host_permissions": [
        "https://{ip}/*",
        "http://{ip}/*"
    ],
    "action": {
        "default_popup": "popup.html"
      },
    "permissions": [
        "activeTab", 
        "scripting"
    ],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content-scripts.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["popup.js", "popup.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
